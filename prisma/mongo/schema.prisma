generator client {
  provider = "prisma-client-js"
  output     = "../../node_modules/prisma-mongo-client"
  engineType = "library"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model Log {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  data String
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  name  String?
}

model Feed {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  userId    String
  posts     Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}